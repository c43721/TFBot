{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "dist",
    "removeComments": false,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "inlineSourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}