{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "outDir": "./dist",
    "sourceMap": false,
    "removeComments": false,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "inlineSourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
  },
  "include": [
    "src/*"
  ],
  "exclude": [
    "node_modules"
  ]
}